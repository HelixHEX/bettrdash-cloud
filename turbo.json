{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["lint"],
      "outputs": ["dist/**"]
    },
    "build-api-gateway": {
      "dependsOn": ["db-generate"],
      "outputs": ["dist/**"]
    },
    "build:desktop": {
      "dependsOn": ["lint", "^db#db:pre-build",  "^build"],
      "outputs": ["dist/**"]
    },
    "build:web": {
      "dependsOn": ["lint", "^db#db:pre-build", "^build"]
    },
    "desktop": {
      "dependsOn": ["db#db:generate"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "db-generate": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "db:pre-build": {
      "outputs": ["dist/**"]
    }
  }
}
